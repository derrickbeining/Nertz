{
  "name": "nertz",
  "version": "1.0.0",
  "description": "A web app bringing the real-time, fast-paced multiplayer solitaire game of Nertz online",
  "engines": {
    "node": "8.9.2"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "git checkout -b deploy && webpack -p && git add -f public/bundle.js public/bundle.js.map && git commit --allow-empty -m 'Deploying' && git push --force heroku deploy:master && git checkout master && git branch -D deploy",
    "start": "node server",
    "start-dev-prev": "npm run build-client-watch & npm run start-server",
    "start-dev": "webpack-dev-server",
    "start-server": "NODE_ENV='development' nodemon server -e html,js,scss --ignore public",
    "test": "NODE_ENV='test' mocha ./client/**/*.spec.js -w --compilers js:babel-register"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "body-parser": "^1.16.1",
    "connect-session-sequelize": "^4.1.0",
    "decked": "^0.1.0",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "firebase": "^4.4.0",
    "history": "^4.6.3",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "react": "^15.4.2",
    "react-dnd": "^2.5.3",
    "react-dnd-html5-backend": "^2.5.3",
    "react-dom": "^15.4.2",
    "react-loading-animation": "^1.4.0",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.3.1",
    "serve-favicon": "^2.4.5",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
